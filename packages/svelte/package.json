{
  "name": "@fmfadigitalisering/staf-komponentbibliotek-svelte",
  "version": "1.2.9",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fmfaDigitalisering/staf-komponentbibliotek-svelte.git"
  },
  "files": [
    "dist"
  ],
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && yarn package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "updateTokens": "cd ../cli && yarn build:tokens"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.1.9",
    "sass": "^1.66.1",
    "semantic-release": "^23.1.1",
    "semantic-release-yarn": "^3.0.2",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@altinn/figma-design-tokens": "^6.0.1",
    "@navikt/aksel-icons": "^5.6.0",
    "altinn-designsystem": "^4.2.0",
    "svelte-floating-ui": "^1.5.5",
    "uuid": "^9.0.1"
  }
}
