{
  "name": "@fmfadigitalisering/staf-komponentbibliotek-svelte",
  "version": "1.7.13",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fmfaDigitalisering/staf-komponentbibliotek-svelte.git"
  },
  "files": [
    "dist"
  ],
  "prettier": {
    "singleQuote": true
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && yarn package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test": "npm run test:integration && npm run test:unit",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "updateTokens": "cd ../cli && yarn build:tokens",
    "storybook:svelte": "storybook dev -p 6006 --config-dir ./storybook --no-open",
    "build:storybook:svelte": "storybook build -o ../../docs --config-dir ./storybook"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "1",
    "@playwright/test": "^1.28.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^10.0.0",
    "@storybook/addon-a11y": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-mdx-gfm": "8.3.0",
    "@storybook/addon-svelte-csf": "^5.0.0-next.3",
    "@storybook/addon-themes": "^8.3.0",
    "@storybook/blocks": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/svelte-vite": "^8.3.0",
    "@storybook/sveltekit": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "@storybook/types": "^8.3.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/package": "^2.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "publint": "^0.1.9",
    "sass": "^1.66.1",
    "semantic-release": "^23.1.1",
    "semantic-release-yarn": "^3.0.2",
    "storybook": "^8.3.0",
    "storybook-css-modules": "^1.0.8",
    "storybook-dark-mode": "^4.0.2",
    "svelte": "^5.0.0-next.244",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.4.1",
    "typescript": "^5.4.5",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@altinn/figma-design-tokens": "^6.0.1",
    "@navikt/aksel-icons": "^5.6.0",
    "svelte-floating-ui": "^1.5.5",
    "uuid": "^9.0.1"
  }
}
