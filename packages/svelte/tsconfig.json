{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "allowJs": true,
    "checkJs": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es6",
    "skipLibCheck": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
